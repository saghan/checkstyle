<checkstyle-metadata>
    <rule-group-metadata name="My custom checks"  priority="1600">
        <description>%MyCustomChecks.desc</description>

        <rule-metadata name="%InterfaceLimit.name" internal-name="InterfaceLimit" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.Ankita.InterfaceLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="2">
                <description>This check, checks for the Swiss Army Knife anti-pattern by checking the number of interfaces.</description>
            </property-metadata>
        </rule-metadata>
        
    </rule-group-metadata>
</checkstyle-metadata>
